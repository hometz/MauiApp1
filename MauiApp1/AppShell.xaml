<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1"
    FlyoutBehavior="Flyout"
    Title="MauiApp1">

    <ShellContent Title="NewPage" ContentTemplate="{ DataTemplate local:NewPage1}" />
    <ShellContent Title="Calculator" ContentTemplate="{ DataTemplate local:MainPage}" />
    <ShellContent Title="ProgressBar" ContentTemplate="{ DataTemplate local:PrgsBar}" />
    <ShellContent Title="DataBase" ContentTemplate="{ DataTemplate local:DataBase}" />
    <ShellContent Title="Rate" ContentTemplate="{ DataTemplate local:RatePage}" />
</Shell>  
